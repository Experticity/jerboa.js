{
  "name": "jerboa",
  "version": "1.0.0",
  "description": "Annotate and provide feedback on any web page.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "uglify": "uglifyjs dist/jerboa.js -c -m -o dist/jerboa.min.js",
    "less": "lessc src/jerboa.less > dist/jerboa.css",
    "autoprefix": "postcss -u autoprefixer -c .postcss.json -o dist/jerboa.css dist/jerboa.css",
    "cssmin": "cssnano dist/jerboa.css dist/jerboa.min.css",
    "build": "webpack; npm run uglify; npm run less; npm run autoprefix; npm run cssmin",
    "build:js": "WEBPACK_STEP=compile webpack; WEBPACK_STEP=uglify webpack"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/camman3d/jerboa.js.git"
  },
  "keywords": [
    "annotate",
    "annotation",
    "feedback",
    "markup",
    "comment"
  ],
  "author": "Josh Monson",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/camman3d/jerboa.js/issues"
  },
  "homepage": "https://github.com/camman3d/jerboa.js",
  "devDependencies": {
    "autoprefixer": "^6.3.7",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "css-loader": "^0.23.1",
    "cssnano": "^3.7.3",
    "cssnano-cli": "^1.0.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "less-loader": "^2.2.3",
    "node-sass": "^3.8.0",
    "postcss-cli": "^2.5.2",
    "postcss-loader": "^0.9.1",
    "precss": "^1.4.0",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "uglify-js": "^2.7.0",
    "webpack": "^1.13.1"
  }
}
